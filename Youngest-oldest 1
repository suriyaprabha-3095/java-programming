import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int age[] = new int[num];
        for(int i=0;i<num;i++){
            age[i] = sc.nextInt();
        }
        Arrays.sort(age);
        System.out.println("Youngest=" + age[0]);
        System.out.println("Oldest=" + age[num-1]);
    }
}
